<script>
	document.addEventListener('DOMContentLoaded', () => {
		const gap = $('.owl-gap')
		const topic = $('.owl-topic')

		gap.owlCarousel({
			nav: false,
			margin: 12,
			loop: true,
			dots: false,
			autoWidth: true,
			slideBy: 2,
			responsive: {
				768: {
