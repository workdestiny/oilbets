<script>
	document.addEventListener('DOMContentLoaded', () => {
		Vue.component('btn-select', {
			props: ['wording', 'id'],
			template: `<div class="col-6 col-md-4 _mgbt-24">
							<button
							type="button"
							class="btn btn-select-topic _bdrd-10px _pdh-12 _w-100pct"
							:class="{'active': checked}"
							@click="check"
							:title="wording"
							>\${wording}</button>
						</div>`,
			data() {
				return {
					checked: false
				}
			},
			methods: {
				check() {
					if (this.checked) {
						this.$emit('un-check', {
							id: this.id,
							name: this.wording
						})
					} else {
						this.$emit('on-check', {
							id: this.id,
							name: this.wording
						})
					}
					this.checked = !this.checked
				}
			}
		}) // component
		new Vue({
			el: '#select-gap',
			data() {
				return {
					listTopic: {{.ListTopic}},
					listSelected: []
				}
			},
			mounted() {
				this.$nextTick(() => {
					$('#select-gap').modal({
						backdrop: 'static',
						keyboard: false
					})
					$('html').css('overflow','hidden')
				})
			},
			computed: {
				count() {
					return this.listSelected.length
				},
				toJson() {
					return JSON.stringify(this.listSelected)
				}
			},
			methods: {
				onSelected(value) {
					this.listSelected.push({
						id: value.id
					})
				},
				unSelected(value) {
					let index = this.listSelected.findIndex(x => x.id === value.id)
					this.listSelected.splice(index, 1)
				},
				submitForm() {
					if (this.count < 2) {
						swal({
							title: 'ต้องเลือกอย่างน้อย 2 หัวข้อ'
						})
						return
					}
					this.$refs.formSelect.submit()
				}
			}
		}) // Vue instant
	})// end

</script>
