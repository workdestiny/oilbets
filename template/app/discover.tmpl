				},
				992: {
					items: 4
				},
				1200: {
					items: 5
				}
			}
		})

		topic.owlCarousel({
			loop: false,
			nav: false,
			dots: false,
			margin: 10,
			autoWidth: true,
			slideBy: 3,
			responsive: {
				768: {
					items: 4
				},
				992: {
					items: 8
				},
				1200: {
					items: 10
				}
			}
		})
		// Go to the next item
		$('.ctrl-carousel.ctrl-gap.next').click(function() {
			gap.trigger('next.owl.carousel', [300])
		})
		// Go to the previous item
